{
	"navigationFallback": {
		"rewrite": "/index.html",
		"exclude": ["/assets/*", "*.svg", "*.png", "*.jpg", "*.webp", "*.ico", "*.js", "*.css", "*.json"]
	},
	"globalHeaders": {
		"Cache-Control": "public, max-age=3600"
	},
	"mimeTypes": {
		".json": "application/json",
		".svg": "image/svg+xml",
		".webp": "image/webp"
	}
}
